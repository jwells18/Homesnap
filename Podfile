# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Homesnap' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Homesnap
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  pod 'Firebase/Storage'
  pod 'SDWebImage'
  pod 'Alamofire', '~> 4.6.0'
  pod 'RealmSwift', '~> 3.0.2'
  pod 'DateToolsSwift'
  pod 'Segmentio', '~> 3.0'

  target 'HomesnapTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'HomesnapUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
